package modelo;

public class Cliente extends Persona {
	private String nroCliente;

	public Cliente(int idPersona, String nombre, String apellido, long dni, String nroCliente) {
		super(idPersona, nombre, apellido, dni);
		// TODO Auto-generated constructor stub
		this.nroCliente=nroCliente;
	}

	public String getNroCliente() {
		return nroCliente;
	}

	public void setNroCliente(String nroCliente) {
		this.nroCliente = nroCliente;
	}

	@Override
	public String toString() {
		return "Cliente [nroCliente=" + nroCliente + ", getIdPersona()=" + getIdPersona() + ", getNombre()="
				+ getNombre() + ", getApellido()=" + getApellido() + ", getDni()=" + getDni() + ", toString()="
				+ super.toString() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
	}
	
	


}
